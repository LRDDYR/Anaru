<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DropDownMenu">
        <attr name="DividerColor" format="color"/><!-- tab分割线颜色 -->
        <attr name="SelectTextColor" format="color"/><!-- 选中的文字颜色 -->
        <attr name="UnSelectTextColor" format="color"/><!-- 未选中的文字颜色 -->
        <attr name="TextSize" format="dimension"/><!-- 文字大小 -->
        <attr name="UnderlineColor" format="color"/><!-- tab下面横线颜色 -->
        <attr name="SelectImage" format="reference"/><!-- 选中的图片 -->
        <attr name="UnSelectImage" format="reference"/><!-- 未选中的图片 -->
        <attr name="MaskColor" format="color"/><!-- 遮罩层颜色 -->
        <attr name="MenuBackgroundColor" format="color"/><!-- 菜单背景颜色 -->
    </declare-styleable>
    <declare-styleable name="RefreshView">
        <attr name="type" format="enum">
            <enum name="sun" value="0" />
        </attr>
    </declare-styleable>
</resources>